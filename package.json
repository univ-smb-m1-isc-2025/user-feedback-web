{
  "name": "user-feedback-web",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "format:check": "prettier --check \"**/*.{ts,html,scss,json}\"",
    "format:check:ci": "prettier --list-different \"**/*.{ts,html,scss,json}\"",
    "format:write": "prettier --write \"**/*.{ts,html,scss,json}\"",
    "lint": "ng lint",
    "lint:fix": "npm run lint -- --fix",
    "test": "jest --colors --maxWorkers=3",
    "test:watch": "npm run test -- --watch",
    "test:coverage": "npm test -- --coverage",
    "prepare": "husky install .husky"
  },
  "engines": {
    "node": ">=18.10.0"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": [
      "npx prettier --write",
      "npx eslint --max-warnings=0 --fix"
    ],
    "*.(html|scss|json)": [
      "npx prettier --write"
    ]
  },
  "private": true,
  "dependencies": {
    "@angular-eslint/builder": "18.3.1",
    "@angular-eslint/eslint-plugin": "18.3.1",
    "@angular-eslint/eslint-plugin-template": "18.3.1",
    "@angular-eslint/template-parser": "18.3.1",
    "@angular/animations": "18.2.7",
    "@angular/cdk": "18.2.7",
    "@angular/common": "18.2.7",
    "@angular/compiler": "18.2.7",
    "@angular/core": "18.2.7",
    "@angular/forms": "18.2.7",
    "@angular/material": "18.2.7",
    "@angular/platform-browser": "18.2.7",
    "@angular/platform-browser-dynamic": "18.2.7",
    "@angular/router": "18.2.7",
    "@jest/globals": "29.7.0",
    "@ngxs/devtools-plugin": "18.1.6",
    "@ngxs/router-plugin": "18.1.6",
    "@ngxs/store": "18.1.6",
    "@stylistic/eslint-plugin-js": "2.9.0",
    "@types/node": "22.7.4",
    "dotenv": "16.4.5",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-unicorn": "56.0.0",
    "jest-junit": "16.0.0",
    "jest-preset-angular": "14.2.4",
    "ng2-charts": "7.0.0",
    "postcss": "8.4.47",
    "rxjs": "7.8.1",
    "tailwindcss": "3.4.13",
    "ts-jest": "29.2.5",
    "tslib": "2.7.0",
    "zone.js": "0.14.10"
  },
  "devDependencies": {
    "@angular-builders/jest": "18.0.0",
    "@angular-devkit/build-angular": "18.2.7",
    "@angular/cli": "18.2.7",
    "@angular/compiler-cli": "18.2.7",
    "@types/jest": "29.5.13",
    "@typescript-eslint/eslint-plugin": "8.8.1",
    "@typescript-eslint/parser": "8.8.1",
    "angular-eslint": "18.3.1",
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-prettier": "5.2.1",
    "eslint-plugin-rxjs": "5.0.3",
    "husky": "9.1.6",
    "jest": "29.7.0",
    "prettier": "3.3.3",
    "prettier-eslint": "16.3.0",
    "typescript": "5.5.4",
    "typescript-eslint": "8.2.0"
  }
}
